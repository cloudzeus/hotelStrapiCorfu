    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      },
      "type": "text"
